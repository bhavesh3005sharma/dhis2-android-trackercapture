<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="table_view_default_all_required_background_color">#091A5A</color>
    <color name="table_view_default_deactivated_background_color">#CECECE</color>
    <color name="table_view_default_disabled_background_color">#5C5C5C</color>
